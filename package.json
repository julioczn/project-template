{
	"name": "project-template",
	"version": "1.0.0",
	"description": "Template for nestjs project",
	"scripts": {
		"prepare": "husky",
		"build": "nest build",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,test}/**/*.ts\" --fix",
		"format": "prettier \"{src,test}/**/*.ts\" --write",
		"type-check": "tsc --noEmit --diagnostics",
		"test": "vitest run --sequence.concurrent --maxConcurrency 10",
		"test:watch": "vitest",
		"test:cov": "vitest run --coverage",
		"test:e2e": "vitest run --config ./vitest.config.e2e.mjs",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false"
	},
	"dependencies": {
		"@anatine/zod-nestjs": "^2.0.12",
		"@nestjs/common": "^9.0.0",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/swagger": "^7.3.1",
		"@nestjs/throttler": "^5.1.2",
		"@prisma/client": "^6.10.1",
		"compression": "^1.7.4",
		"dd-trace": "^5.17.0",
		"helmet": "^7.1.0",
		"nestjs-pino": "^4.1.0",
		"pino-http": "^10.1.0",
		"pino-pretty": "^11.2.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@swc/cli": "^0.1.63",
		"@swc/core": "^1.3.102",
		"@types/compression": "^1.7.5",
		"@types/eslint": "^8.56.1",
		"@types/eslint-plugin-prettier": "^3.1.3",
		"@types/express": "^4.17.17",
		"@types/node": "^20.14.2",
		"@types/prettier": "^3.0.0",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"@typescript-eslint/typescript-estree": "^6.17.0",
		"@vitest/coverage-v8": "^1.6.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.6",
		"openapi-typescript": "^6.7.6",
		"prettier": "^3.3.2",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5",
		"typescript-transform-paths": "^3.4.7",
		"unplugin-swc": "^1.4.5",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0",
		"vitest-fetch-mock": "^0.2.2"
	},
	"engines": {
		"node": "20"
	}
}
